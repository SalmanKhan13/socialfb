<%- layout('layouts/main') %>
<div class="row p-2">
  <div class="col-md-3">
    <div class="card">
    </div>
  </div>
  <div class="col-md-9">
    <div class="row">
      <% resp.data.forEach(function( data ) { %>
      <div class="col-md-4">
        <div class="card mb-2 animated bounceInUp">
          <img src="<%= data.media_url %>" class="card-img-top" />
          <div class="card-body">
            <p>Caption <%= data.caption %></p>
            <p>Timestamp</p>
            <p class="text-muted"><%= data.timestamp %></p>
            <p>Likes_Count</p>
            <p class="text-muted"><%= data.likes_count %></p>
            <p>MediaType</p>
            <p class="text-muted"><%= data.media_type %></p>
            <p>Comments_Count</p>
            <p class="text-muted"><%= data.comments_count %></p>

          </div>
        </div>
      </div>
      <% })%>
    </div>
  </div>
</div>